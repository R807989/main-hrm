pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout your Angular project
                git 'https://github.com/R807989/main-hrm.git'
            }
        }

        stage('Install Dependencies') {
    steps {
        // Use the installed Node.js version
        withNodejs('NodeJS') {
            // Install npm dependencies
            sh 'npm install'
        }
    }
}


        // Add other stages as needed
    }
}
