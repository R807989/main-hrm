pipeline {
agent any 

stages{
stage("intigration"){
steps{


git 'https://github.com/R807989/main-hrm.git'





 }

}
stage('Install Dependencies') {
            steps {
                script {
                    sh 'npm install'
                
            
    
}
}
}
stage('build'){
steps {
                dir('/var/lib/jenkins/workspace/s')
 {
                    script {
                        sh 'ng build'
                    }
                }
            }

}
}

}
