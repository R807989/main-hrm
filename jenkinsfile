pipeline {
agent any 

stages{
stage("intigration"){
steps{


git 'https://github.com/R807989/main-hrm.git'





 }

}
stage('Install Dependencies') {


            steps {
dir ("dist/template"){

}
            script {
                    sh 'npm install'
                
            
    
}
}
}
stage('build'){
steps {
                dir("dist/template")
 {
                    script {
                        sh 'ng build'
                    }
                }
            }

}
}

}
