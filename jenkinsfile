pipeline {

agent any

environment {
        // Set up the Node.js tool
        NODEJS_HOME = tool 'nodeJS'
        PATH = "${NODEJS_HOME}/bin:${env.PATH}"a
    }


stages{
stage("intigration"){

steps{

git 'https://github.com/R807989/main-hrm.git'

}
}
stage("install dependcies"){
steps{
dir ('pro') {
script {
                   
                    sh 'npm install'
                }
}

}



}
}


}
