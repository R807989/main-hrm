pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout your Angular project
                git 'https://github.com/R807989/main-hrm.git'
            }
        }

        stage('Install Dependencies') {
            steps {
                // Install npm dependencies
                script {
                    sh 'npm install'
                }
            }
        }

        // Add other stages as needed
    }
}
