pipeline {

agent any 


 stages {
         








stage ("git intigration"){


steps{

  git 'https://github.com/R807989/main-hrm.git'


}
}





stage("build image") {
steps{
sh 'docker build -t hrms-frontend .'

  }

}


stage("run HRMS image") {
            steps {
                script {
                    // Replace 'your_script_to_run_hrms_image.sh' with the actual script or command
                    sh 'docker run -d -p 4200:4200 hrms-frontend'
                }
            }
        }


}

}



