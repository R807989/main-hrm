pipeline {

agent any 


 stages {
         








stage ("git intigration"){


steps{

  git 'https://github.com/R807989/main-hrm.git'


}
}


stage("Check npm version") {
    steps {
        sh 'npm --version'
    }
}



stage("build") {
    steps {
        sh 'docker build -t hrms-frontend .'
    }
}

stage("deploy"){
steps{

docker run -it --name hrms-container hrms-frontend /bin/bash


}


}




}
}


