pipeline {

agent any 


 stages {
         








stage ("git intigration"){


steps{

  git 'https://github.com/R807989/main-hrm.git'


}
}


stage("Check npm version") {
    steps {
        sh 'npm --version'
    }
}



stage("build") {
    steps {
        sh 'docker build -t hrms-frontend .'
    }
}
stage("run"){

steps {


}
}

stage("Access Container Shell") {
            steps {
                script {
                    // Run a shell inside the running container
                    sh 'docker exec -it hrms-container /bin/bash'
                }
            }
}


}
}


