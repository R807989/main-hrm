pipeline {

agent any 


 stages {
         








stage ("git intigration"){


steps{

  git 'https://github.com/R807989/main-hrm.git'


}
}





stage("build image") {
steps{
sh 'docker build -t hrms-frontend .'

  }

}

stage ('Docker Run') {
steps {
        sh 'docker run -p 4200:4200 hrms-frontend'        
    }
}




}

}



